#include<bits/stdc++.h> 
int a[8];
using namespace std;

int main( )
{
    double s,r;
    for(int i = 0; i < 6; i++)
    {
        scanf("%d",&a[i]);
    }
    s = sqrt(pow(abs(a[0]-a[3]),2)+pow(abs(a[1]-a[4]),2));
    r = sqrt(pow(s,2)+pow(abs(a[2]-a[5]),2));
    printf("%.2lf",r);
    return 0;
}
